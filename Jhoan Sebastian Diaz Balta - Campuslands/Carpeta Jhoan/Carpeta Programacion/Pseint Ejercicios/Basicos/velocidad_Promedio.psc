Proceso velocidad_promedio
	Definir distancia, tiempo, resultado Como Real;
	
	Escribir "Ingrese la distancia recorrida: ";
	Leer distancia;
	
	Escribir "Ingrese el tiempo en horas: ";
	Leer tiempo;
	
	resultado <- distancia / tiempo;
	
	Escribir "La velocidad promedio es: ",resultado;
	
FinProceso
